{
	"name": "starter-kit-expo",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"web": "expo start --web",
		"android": "expo run:android",
		"android:run": "eas build:run --profile development --platform android --latest && expo start --dev-client",
		"android:build": "eas build --profile development --platform android",
		"ios": "expo run:ios",
		"ios:build": "eas build --profile development --platform ios",
		"prebuild": "echo y | expo prebuild --clean",
		"tsc": "tsc --noEmit",
		"install-fix": "expo install --fix",
		"doctor": "npx expo-doctor",
		"clean-build": "rm -rf android ios .expo",
		"clean-node": "rm -rf package-lock.json bun.lock yarn.lock node_modules",
		"clean-all": "npm run clean-build && npm run clean-node",
		"test": "jest --watchAll",
		"lint": "eslint .",
		"format": "eslint . --fix"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo-google-fonts/inter": "^0.4.2",
		"@expo/html-elements": "^0.12.5",
		"@expo/vector-icons": "^15.0.2",
		"@gluestack-ui/core": "^3.0.12",
		"@gluestack-ui/utils": "^3.0.13",
		"@gorhom/bottom-sheet": "^5.0.0-alpha.11",
		"@legendapp/motion": "^2.4.0",
		"@react-aria/utils": "^3.29.0",
		"@react-native-masked-view/masked-view": "0.3.2",
		"@react-navigation/native": "^7.1.6",
		"dom-helpers": "^5.2.1",
		"expo": "~54.0.30",
		"expo-blur": "~15.0.8",
		"expo-constants": "~18.0.12",
		"expo-dev-client": "~6.0.20",
		"expo-font": "~14.0.8",
		"expo-linear-gradient": "~15.0.8",
		"expo-linking": "~8.0.11",
		"expo-router": "~6.0.21",
		"expo-splash-screen": "~31.0.13",
		"expo-status-bar": "~3.0.8",
		"expo-system-ui": "~6.0.7",
		"expo-web-browser": "~15.0.7",
		"jotai": "^2.15.2",
		"lucide-react-native": "^0.510.0",
		"moti": "^0.30.0",
		"nativewind": "^4.2.1",
